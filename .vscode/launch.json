{
  "configurations": [
    {
      "name": "(gdb) Launch outerface_test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/src/outerface_interface/outerface_test",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/build/src/outerface",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    }
  ]
}